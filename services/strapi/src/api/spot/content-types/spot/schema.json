{
  "kind": "collectionType",
  "collectionName": "spots",
  "info": {
    "singularName": "spot",
    "pluralName": "spots",
    "displayName": "観光スポット",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "spot_id_originals": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::original-course.original-course",
      "inversedBy": "spots"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "spotIds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::model-course.model-course",
      "inversedBy": "spots"
    },
    "postCode": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "contact": {
      "type": "string"
    },
    "businessHours": {
      "type": "string"
    },
    "holidayIds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::holiday.holiday",
      "mappedBy": "spotIds"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "categoryIds"
    }
  }
}
